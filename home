    private void jButton9ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        if (searchfield1.getText().isEmpty()) {
            JOptionPane.showMessageDialog(this, "Please Enter student Register Number ");

        } else {
            jTable2.setModel(new DefaultTableModel(null, new Object[]{"name", "reg_no", "dept", "gender", "date_of_birth", "father_name", "mother_name", "address", "phone_no", "academic", "total_amt", "amt_paid", "paid_date", "bal_amt"}));
            student.getStudentvalue(jTable2,searchfield1.getText());

        }
    }                                        

    private void jButton10ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        jTable2.setModel(new DefaultTableModel(null, new Object[]{"name", "reg_no", "dept", "gender", "date_of_birth", "father_name", "mother_name", "address", "phone_no", "academic", "total_amt", "amt_paid", "paid_date", "bal_amt"}));
            student.getStudentvalue(jTable2, "");
            searchfield1.setText(null);
            
    }                                         

    private void jButton11ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        if (isEmptyStudent()) {
            String name = jTextField13.getText();
            int reg_no = student.getMax();
            String dept = jComboBox3.getSelectedItem().toString();
            String gender = jComboBox4.getSelectedItem().toString();
            String date_of_birth = jTextField15.getText();
            String father_name = jTextField16.getText();
            String mother_name = jTextField17.getText();
            String address = jTextField18.getText();
            String phone_no = jTextField19.getText();
            String academic = jTextField20.getText();
            String total_amt = jTextField21.getText();
            String amt_paid = jTextField22.getText();
            String paid_date = jTextField23.getText();
            String bal_amt = jTextField24.getText();
            student.insert(name, reg_no, dept, gender, date_of_birth, father_name, mother_name, address, phone_no, academic, total_amt, amt_paid, paid_date, bal_amt);
            clearStudent();
        }
    }                                         

    private void jButton16ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        int a = JOptionPane.showConfirmDialog(this, "Do You Want To Logout Now?", "Select", JOptionPane.YES_NO_OPTION);
        if (a == 0) {
            new login().setVisible(true);
            this.dispose();
        }
    }                                         

    private void jPanel1MouseDragged(java.awt.event.MouseEvent evt) {                                     
        int x = evt.getXOnScreen();
        int y = evt.getYOnScreen();
        this.setLocation(x - xx, y - xy);
    }                                    

    private void jPanel1MousePressed(java.awt.event.MouseEvent evt) {                                     
        xx = evt.getX();
        xy = evt.getY();
    }                                    

    private void formWindowOpened(java.awt.event.WindowEvent evt) {                                  
        for (double i = 0.1; i <= 1.0; i += 0.1) {
            String s = i + "";
            float f = Float.valueOf(s);
            this.setOpacity(f);
            try {
                Thread.sleep(40);
            } catch (InterruptedException ex) {
                Logger.getLogger(home.class.getName()).log(Level.SEVERE, null, ex);
            }
        }
    }                                 

    private void jButton15ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        clearStudent();
    }                                         

    private void jTextField19KeyTyped(java.awt.event.KeyEvent evt) {                                      
        if (!Character.isDigit(evt.getKeyChar())) {
            evt.consume();
        }
    }                                     

    private void jButton12ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        if (isEmptyStudent()) {
            String name = jTextField13.getText();
            int reg_no = Integer.parseInt(jTextField14.getText());
            String dept = jComboBox3.getSelectedItem().toString();
            String gender = jComboBox4.getSelectedItem().toString();
            String date_of_birth = jTextField15.getText();
            String father_name = jTextField16.getText();
            String mother_name = jTextField17.getText();
            String address = jTextField18.getText();
            String phone_no = jTextField19.getText();
            String academic = jTextField20.getText();
            String total_amt = jTextField21.getText();
            String amt_paid = jTextField22.getText();
            String paid_date = jTextField23.getText();
            String bal_amt = jTextField24.getText();
            student.update(name, reg_no, dept, gender, date_of_birth, father_name, mother_name, address, phone_no, academic, total_amt, amt_paid, paid_date, bal_amt);
            clearStudent();

        }
    }                                         

    private void jTable2MouseClicked(java.awt.event.MouseEvent evt) {                                     
        model = (DefaultTableModel) jTable2.getModel();
        rowIndex = jTable2.getSelectedRow();
        jTextField13.setText(model.getValueAt(rowIndex, 0).toString());
        jTextField14.setText(model.getValueAt(rowIndex, 1).toString());
        String dept = model.getValueAt(rowIndex, 2).toString();
        if (dept.equals("CSE")) {
            jComboBox3.setSelectedIndex(0);
        } else if (dept.equals("IT")) {
            jComboBox3.setSelectedIndex(1);
        } else if (dept.equals("CSE-CS")) {
            jComboBox3.setSelectedIndex(2);
        } else if (dept.equals("BME")) {
            jComboBox3.setSelectedIndex(3);
        } else if (dept.equals("MECH")) {
            jComboBox3.setSelectedIndex(4);
        } else if (dept.equals("ECE")) {
            jComboBox3.setSelectedIndex(5);
        } else if (dept.equals("EEE")) {
            jComboBox3.setSelectedIndex(6);
        } else {
            jComboBox3.setSelectedIndex(7);
        }
        String gender = model.getValueAt(rowIndex, 3).toString();
        if (gender.equals("MALE")) {
            jComboBox4.setSelectedIndex(0);
        } else {
            jComboBox4.setSelectedIndex(1);
        }
        jTextField15.setText(model.getValueAt(rowIndex, 4).toString());
        jTextField16.setText(model.getValueAt(rowIndex, 5).toString());
        jTextField17.setText(model.getValueAt(rowIndex, 6).toString());
        jTextField18.setText(model.getValueAt(rowIndex, 7).toString());
        jTextField19.setText(model.getValueAt(rowIndex, 8).toString());
        jTextField20.setText(model.getValueAt(rowIndex, 9).toString());
        jTextField21.setText(model.getValueAt(rowIndex, 10).toString());
        jTextField22.setText(model.getValueAt(rowIndex, 11).toString());
        jTextField23.setText(model.getValueAt(rowIndex, 12).toString());
        jTextField24.setText(model.getValueAt(rowIndex, 13).toString());

    }                                    

    private void jButton13ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        int reg_no = Integer.parseInt(jTextField14.getText());
        if (student.isreg_noExist(reg_no)) {
            student.delete(reg_no);
            jTable2.setModel(new DefaultTableModel(null, new Object[]{"name", "reg_no", "dept", "gender", "date_of_birth", "father_name", "mother_name", "address", "phone_no", "academic", "total_amt", "amt_paid", "paid_date", "bal_amt"}));
            student.getStudentvalue(jTable2, "");
            clearStudent();
        } else {
            JOptionPane.showMessageDialog(this, "Register Number is doesn't Exist");

        }
    }                                         

